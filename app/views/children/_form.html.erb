<div class="row" id="child-form">
  <div class="col-md-10 col-lg-6 mx-auto my-3">
    <%= form_with model: child, url:user_children_path(user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name, "お子さま登録フォーム", class: "form-label" %>
      <%= f.text_field :name, class: "form-control mb-2", placeholder: "ニックネームを入力" %>
      <%= f.submit class: "btn btn-primary custom-btn" %>
    <% end %>
  </div>
</div>
